{
    "name": "personal-portfolio",
    "version": "1.0.0",
    "description": "Personal development, learning and playground",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "scss": "node-sass --watch src/scss -o src/css",
        "prefix": "postcss dist/css/main.css --use=autoprefixer --map=false --output=dist/css/main.css",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production && npm run prefix",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "author": "Queeny Kenn Amar",
    "license": "ISC",
    "dependencies": {
        "bootstrap": "^4.4.1",
        "bulma": "^0.8.2",
        "jquery": "^3.4.1",
        "js-loading-overlay": "^1.0.4"
    },
    "devDependencies": {
        "autoprefixer": "^9.7.6",
        "clean-css-cli": "^4.3.0",
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.4",
        "node-sass": "^4.13.1",
        "popper.js": "^1.12",
        "postcss-cli": "^7.1.0",
        "postcss-loader": "^2.1.4",
        "resolve-url-loader": "^5.0.0",
        "sass-loader": "^8.0.2",
        "vue-template-compiler": "^2.6.11"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "last 1 ie version"
        ]
    }
}
